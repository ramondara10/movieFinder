{
  "name": "moviefinder",
  "version": "1.0.0",
  "description": "Final Project 3",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "build": "cd client && npm run build",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramondara10/movieFinder.git"
  },
  "author": "Ramon Alvarado",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ramondara10/movieFinder/issues"
  },
  "homepage": "http://ramondara10.github.io/movieFinder",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "build": "^0.1.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.13.2",
    "movie-trailer": "^2.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-search-field": "^2.0.1",
    "react-text-truncate": "^0.17.0",
    "react-youtube": "^7.13.1",
    "require-stack": "^1.0.2",
    "start": "^5.1.0",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
